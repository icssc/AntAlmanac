{
    "name": "antalmanac-backend",
    "version": "0.0.1",
    "description": "Backend for AntAlmanac",
    "type": "module",
    "scripts": {
        "dev": "vite build --watch --mode development",
        "build": "vite build",
        "start": "concurrently 'npm run dev' 'nodemon dist'",
        "format": "prettier --write .",
        "lint": "eslint ."
    },
    "dependencies": {
        "@prisma/client": "^4.10.1",
        "@trpc/server": "^10.23.0",
        "@typegoose/typegoose": "^11.1.0",
        "@vendia/serverless-express": "^4.10.1",
        "aws-lambda": "^1.0.7",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "mongodb": "^5.0.1",
        "mongoose": "^7.1.0j",
        "superjson": "^1.12.3",
        "websoc-api": "^3.0.0",
        "zod": "^3.20.6"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.110",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@typescript-eslint/eslint-plugin": "^5.52.0",
        "@typescript-eslint/parser": "^5.52.0",
        "aws-sdk": "^2.1368.0",
        "concurrently": "^8.0.1",
        "eslint": "^8.34.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-import": "^2.27.5",
        "husky": "^8.0.3",
        "lint-staged": "^13.1.2",
        "nodemon": "^2.0.22",
        "prettier": "^2.8.4",
        "prisma": "^4.10.1",
        "tsx": "^3.12.3",
        "typescript": "^4.9.5",
        "vite": "^4.1.1",
        "vite-plugin-node": "^3.0.2"
    },
    "lint-staged": {
        "*.{js,json,css,html}": [
            "prettier --write",
            "git add"
        ]
