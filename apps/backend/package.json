{
    "name": "antalmanac-backend",
    "version": "0.0.1",
    "description": "Backend for AntAlmanac",
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "get-data": "tsx scripts/fetch-term-data.ts && tsx scripts/get-search-data.ts",
        "copy-terms-to-dist": "tsx scripts/copy-terms-to-dist.ts",
        "zip-dist": "tsx scripts/zip-dist.ts",
        "prebuild": "pnpm get-data && pnpm copy-terms-to-dist",
        "build": "node scripts/build.mjs && pnpm zip-dist",
        "prestart": "pnpm get-data",
        "start": "pnpm dev",
        "format": "prettier --write src",
        "lint": "eslint --fix src",
        "studio": "drizzle-kit studio",
        "migrate": "drizzle-kit generate && tsx scripts/migrate.ts"
    },
    "dependencies": {
        "@leeoniya/ufuzzy": "1.0.14",
        "@packages/antalmanac-types": "workspace:*",
        "@paralleldrive/cuid2": "^2.2.2",
        "@trpc/server": "^10.30.0",
        "@vendia/serverless-express": "^4.10.1",
        "archiver": "^6.0.2",
        "arctic": "^3.7.0",
        "arktype": "1.0.14-alpha",
        "aws-lambda": "^1.0.7",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "drizzle-orm": "^0.36.3",
        "envalid": "^7.3.1",
        "express": "^4.18.2",
        "fuzzysort": "3.1.0",
        "google-auth-library": "^9.15.0",
        "postgres": "^3.4.4",
        "superjson": "^1.12.3",
        "zod": "4.1.12"
    },
    "devDependencies": {
        "@types/archiver": "^6.0.3",
        "@types/aws-lambda": "^8.10.110",
        "@types/cookie-parser": "^1.4.10",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@typescript-eslint/eslint-plugin": "^5.52.0",
        "@typescript-eslint/parser": "^5.52.0",
        "concurrently": "^8.0.1",
        "dotenv": "^16.0.3",
        "drizzle-kit": "^0.28.1",
        "esbuild": "^0.17.19",
        "eslint": "^8.34.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-import": "^2.27.5",
        "husky": "^8.0.3",
        "lint-staged": "^13.1.2",
        "nodemon": "^2.0.22",
        "prettier": "^2.8.4",
        "tsx": "^4.19.2",
        "typescript": "5.6.3"
    },
    "lint-staged": {
        "*.{js,json,css,html}": [
            "prettier --write"
        ]
    }
}
