{
    "name": "antalmanac",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/icssc/Antalmanac"
    },
    "scripts": {
        "_build": "turbo build --env-mode=loose",
        "_build:aa": "turbo build --filter=antalmanac --env-mode=loose",
        "_deploy": "turbo deploy",
        "_deploy:aa": "turbo deploy --filter=antalmanac",
        "_deploy:cdk": "turbo deploy --filter=cdk",
        "_prepare": "husky install",
        "_start": "cross-env VITE_LOCAL_SERVER=true turbo start",
        "_start:aa": "turbo start --filter=antalmanac",
        "test": "vitest",
        "deploy": "sst deploy --stage production"
    },
    "dependencies": {
        "sst": "3.17.19"
    },
    "devDependencies": {
        "@types/aws-lambda": "8.10.156",
        "@types/eslint": "^8.56.5",
        "@types/node": "^20.11.6",
        "cross-env": "^7.0.3",
        "eslint-import-resolver-typescript": "^3.6.1",
        "husky": "^8.0.3",
        "jsdom": "^22.1.0",
        "lint-staged": "^13.1.1",
        "prettier": "^3.1.0",
        "tsx": "^4.19.2",
        "turbo": "latest",
        "vitest": "^0.34.4"
    },
    "packageManager": "pnpm@10.22.0",
    "engines": {
        "node": ">=22",
        "pnpm": "^10.0.0"
    },
}
