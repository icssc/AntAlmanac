{
    "name": "antalmanac",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/icssc/Antalmanac"
    },
    "scripts": {
        "prepare": "husky install",
        "dev": "pnpm --filter antalmanac dev",
        "db:generate": "pnpm --filter db db:generate",
        "db:migrate": "pnpm --filter db db:migrate",
        "db:studio": "pnpm --filter db db:studio",
        "test": "vitest",
        "deploy": "sst deploy --stage production"
    },
    "devDependencies": {
        "@types/eslint": "^8.56.5",
        "@types/node": "^20.11.6",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.3",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.7.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.27.5",
        "husky": "^8.0.3",
        "jsdom": "^22.1.0",
        "lint-staged": "^13.1.1",
        "prettier": "^3.1.0",
        "sst": "3.17.25",
        "tsx": "^4.19.2",
        "vitest": "^0.34.4"
    },
    "packageManager": "pnpm@10.22.0",
    "engines": {
        "node": ">=22",
        "pnpm": "^10.0.0"
    },
    "pnpm": {
        "overrides": {
            "@types/react": "19.2.7",
            "@types/react-dom": "19.2.3",
            "react-is": "^19.0.0"
        }
    }
}
