{
    "name": "antalmanac",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/icssc/Antalmanac"
    },
    "scripts": {
        "prepare": "husky install",
        "dev": "pnpm --filter antalmanac dev",
        "db:generate": "pnpm --filter db db:generate",
        "db:migrate": "pnpm --filter db db:migrate",
        "db:studio": "pnpm --filter db db:studio",
        "test": "vitest",
        "deploy": "sst deploy --stage production",
        "format": "oxfmt",
        "lint": "oxlint --deny-warnings"
    },
    "devDependencies": {
        "@types/node": "^20.11.6",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.3",
        "husky": "^8.0.3",
        "jsdom": "^22.1.0",
        "lint-staged": "^13.1.1",
        "oxfmt": "^0.34.0",
        "oxlint": "^1.49.0",
        "sst": "3.17.25",
        "tsx": "^4.19.2",
        "vitest": "^0.34.4"
    },
    "packageManager": "pnpm@10.22.0",
    "engines": {
        "node": ">=22",
        "pnpm": "^10.0.0"
    },
    "pnpm": {
        "overrides": {
            "@types/react": "19.2.7",
            "@types/react-dom": "19.2.3",
            "react-is": "^19.0.0"
        }
    }
}